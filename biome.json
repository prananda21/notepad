{
  "$schema": "https://biomejs.dev/schemas/2.2.4/schema.json",
  "vcs": {
    "enabled": true,
    "clientKind":"git",
    "useIgnoreFile": true
  },
  "files": {
    "ignoreUnknown": false
  },
  "extends": ["ultracite"],
  "formatter": {
    "enabled": true,
    "indentStyle": "tab"
    
  },
  "linter": {
    "enabled":true,
    "rules": {
      "recommended": true,
      "performance":"info"
    }
  }
  
}
